{
    "name": "playplay/laravel-admin-package",
    "description": "laravel-admin-package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.0",
        "uxweb/sweet-alert": "^2.0",
        "laravelcollective/html": "^6.0",
        "watson/bootstrap-form": "^1.1",
        "spatie/laravel-menu": "^3.0",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelAdminPackage\\": "src/",
            "LaravelAdminPackage\\App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAdminPackage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
